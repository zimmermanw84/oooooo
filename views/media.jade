extends layout

block content

  a(class='btn btn-warning', href='/dashboard', style='margin-bottom: 2em;') Back

  ul
    for img in imgs
      li
        a(href='/media/#{img.image_id}')
          img(src='#{img.link}', style='max-width:5em;max-height:5em;')

        h4 Uploaded By: &nbsp;
          = img.user.username